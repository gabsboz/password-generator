{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Generates secure passwords",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  